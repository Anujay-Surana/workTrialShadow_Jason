# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
REDIRECT_URI=http://localhost:8080/auth/google/callback

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_service_role_key_here

# Parallel Processing Configuration
# Maximum workers per user (prevents single user from monopolizing resources)
MAX_WORKERS_PER_USER=5
# Maximum total workers across all users
MAX_TOTAL_WORKERS=5

# Debug Mode
# When true, processes only 50 most recent files (for faster testing)
DEBUG_MODE=true

# Search Configuration
# Number of top results to return from combined search
SEARCH_TOP_K=10

# Output Configuration
# Enable verbose logging for debugging
VERBOSE_OUTPUT=false
